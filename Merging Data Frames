cus_id <- c(1,2,3,4)
name_vc<- c("Karthik","Ganesh","Saraswati","Laxmi")
prof<- c("Doctor","Engineer","Lawyer","Painter")
marks<- c(12,13,14,15)
loc_vc <- c("A","B","C","D")
year<- c(2006,2007,2008,2009)
data1_df<- data.frame(cus_id,name_vc,prof,marks)
data2_df <- data.frame(cus_id,loc_vc,year)
View(data1_df)
View(data2_df)
final_df<- merge(data1_df,data2_df,by=c("cus_id"))
View(final_df)
